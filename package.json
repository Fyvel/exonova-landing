{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "postbuild": "npm run sitemap:au && npm run sitemap:global && npm run copy-sitemaps",
    "sitemap:au": "next-sitemap --config next-sitemap-au.config.js",
    "sitemap:global": "next-sitemap --config next-sitemap-global.config.js",
    "copy-sitemaps": "cp public/sitemaps-au/sitemap.xml public/sitemap-au.xml && cp public/sitemaps-global/sitemap.xml public/sitemap-global.xml",
    "postcopy-sitemaps": "rm -rf public/sitemaps-au && rm -rf public/sitemaps-global",
    "start": "next start",
    "lint:js": "next lint",
    "lint:css": "stylelint 'src/**/*.{css,scss}'",
    "lint": "npm run lint:js -- --fix && npm run lint:css -- --fix"
  },
  "dependencies": {
    "next": "15.5.3",
    "next-themes": "0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3",
    "@eslint/js": "9.35.0",
    "@types/node": "20",
    "@types/react": "19",
    "@types/react-dom": "19",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
    "eslint": "9.35.0",
    "eslint-config-next": "15.4.6",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "next-sitemap": "^4.2.3",
    "stylelint": "16.23.1",
    "stylelint-config-standard": "39.0.0",
    "typescript": "5",
    "typescript-eslint": "8.43.0"
  }
}